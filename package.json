/* Fix layout issues */
.grid-container {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
}

/* Add animations */
@keyframes fadeIn {
  from { opacity: 0; }
  to { opacity: 1; }
}

/* Improve accessibility */
a:focus {
  outline: 2px solid #007bff;
  outline-offset: 2px;
}
